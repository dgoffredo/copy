#!/bin/sh

cat <<END_SIZES
4K $((1024 * 4)) 4K x 1
16K $((1024 * 16)) 16K x 1
64K $((1024 * 64)) 64K x 1
512K $((1024 * 512)) 512K x 1
1M $((1024 * 1024 * 1)) 1M x 1
2M $((1024 * 1024 * 2)) 2M x 1
8M $((1024 * 1024 * 8)) 8M x 1
64M $((1024 * 1024 * 64)) 64M x 1
256M $((1024 * 1024 * 256)) 256M x 1
512M $((1024 * 1024 * 512)) 512M x 1
1G $((1024 * 1024 * 1024 * 1)) 1G x 1
4G $((1024 * 1024 * 1024 * 4)) 1G x 4
8G $((1024 * 1024 * 1024 * 8)) 1G x 8
END_SIZES
